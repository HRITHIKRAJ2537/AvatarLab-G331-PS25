generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Avatar {
  id     String @id
  name   String
  gender String
  video  Bytes
}

model Voice {
  id       String @id
  name     String
  text     String
  gender   String
  audio    Bytes
  language String
}
